<div class="modal fade" role="dialog" id="confirmDelete">
    <div class="modal-dialog" role="document">
        <div class="modal-content" role="document">
            <div class="modal-header">
                <h5 class="modal-header">Confirm Delete</h5>
                <button class="close" type="button" data-dismiss="modal"><span class="fas fa-times-circle"></span></button>
            </div>
            <div class="modal-body">
                <p>Are you sure you want to delete this <span id="objectName">@ViewData["ObjectName"]</span>?</p>
            </div>
            <div class="modal-footer">
                <button id="cancelDelete" type="button" class="btn btn-outline-primary" onclick="$('#confirmDelete').modal('hide');"><span class="fas fa-times"></span> No, go back</button>
                <a id="deleteButton" href="#" class="btn btn-primary"><span class="fas fa-trash"></span> Yes, delete</a>
            </div>
        </div>
    </div>
</div>
